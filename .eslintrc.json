{
  "parser": "@babel/eslint-parser",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "prettier/react",
    "./.eslintrc.rules.json"
  ],
  "plugins": ["prettier", "react", "react-hooks", "jsx-a11y", "import"],
  "env": { "browser": true, "es6": true },
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
